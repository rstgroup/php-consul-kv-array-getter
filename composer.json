{
    "name": "rstgroup/php-consul-kv-array-getter",
    "description": "This library allows you to receive the consul's KV Store's keys as a structured array.",
    "authors": [
        {
            "name": "Artur Wolny",
            "email": "artur.wolny@rst.com.pl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "sensiolabs/consul-php-sdk": "^3",
        "php": ">= 5.6"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "RstGroup\\PhpConsulKVArrayGetter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RstGroup\\PhpConsulKVArrayGetter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "unit-tests": "phpunit --bootstrap=vendor/autoload.php tests/"
    }
}
